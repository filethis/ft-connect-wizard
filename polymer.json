{
  "entrypoint": "ft-connect-wizard.html",
  "shell": "ft-connect-wizard.html",
  "fragments": [
  ],
  "sources": [
    "*.html"
  ],
  "extraDependencies": [
    "manifest.json"
  ],
  "lint": {
    "rules": [
      "polymer-2-hybrid"
    ],
    "ignoreWarnings": [
      "multiple-doc-comments"
    ]
  },
  "builds": [
    {
      "name": "dropin",
      "bundle": true,
      "js": {
        "minify": true,
        "compile": false
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "excludes": ["/data/"],
      "stripComments": "true",
      "addPushManifest": true,
      "insertPrefetchLinks": true
    }
  ]
}
