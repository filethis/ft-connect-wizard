{
  "npm": true,
  "shell": "ft-connect-wizard.js",
  "fragments": [],
  "sources": [
    "*.js",
    "*.html"
  ],
  "lint": {
    "rules": [
      "polymer-3"
    ]
  },
  "builds": [
    {
      "name": "prod",
      "browserCapabilities": [
        "es2015"
      ],
      "js": {
        "minify": false,
        "compile": false
      },
      "css": {
        "minify": false
      },
      "html": {
        "minify": false
      },
      "bundle": false,
      "addServiceWorker": true,
      "addPushManifest": false
    }
  ]
}