{
  "entrypoint": "ft-connect-wizard.html",
  "shell": "ft-connect-wizard.html",
  "fragments": [],
  "sources": [
    "*.html",
    "data/**/*",
    "ft-connect-wizard.html",
    "ft-connect-wizard.html"
  ],
  "extraDependencies": [
    "bower_components/webcomponentsjs/*.js",
    "manifest.json",
    "bower_components/webcomponentsjs/webcomponents-lite.js"
  ],
  "builds": [
    {
      "name": "dropin",
      "bundle": true,
      "js": {
        "minify": true,
        "compile": false
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "excludes": [
        "/data/"
      ],
      "stripComments": "true",
      "addPushManifest": true,
      "insertPrefetchLinks": true
    }
  ],
  "lint": {
    "rules": [
      "polymer-2-hybrid"
    ],
    "warningsToIgnore": [
      "multiple-doc-comments"
    ]
  }
}